<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane prefHeight="160.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AddGoalController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="15.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="237.0" minWidth="10.0" prefWidth="58.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="330.0" minWidth="10.0" prefWidth="92.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="330.0" minWidth="10.0" prefWidth="13.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="330.0" minWidth="10.0" prefWidth="118.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="331.0" minWidth="10.0" prefWidth="106.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="15.0" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="15.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="15.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label text="Volume" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label text="Points" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <TextField fx:id="volumeField" prefHeight="25.0" prefWidth="11.0" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      <TextField fx:id="pointsField" GridPane.columnIndex="2" GridPane.rowIndex="2" />
      <Button fx:id="addButton" mnemonicParsing="false" onMouseClicked="#addButtonClicked" text="Add" GridPane.columnIndex="5" GridPane.rowIndex="1" />
      <Button fx:id="resetButton" mnemonicParsing="false" onMouseClicked="#resetButtonClicked" text="Reset" GridPane.columnIndex="5" GridPane.rowIndex="2" />
      <Button fx:id="exitButton" mnemonicParsing="false" onMouseClicked="#exitButtonClicked" text="Exit" GridPane.columnIndex="5" GridPane.rowIndex="3" />
      <Label fx:id="volumeErrorLabel" GridPane.columnIndex="4" GridPane.rowIndex="1" />
      <Label fx:id="pointsErrorLabel" GridPane.columnIndex="4" GridPane.rowIndex="2" />
   </children>
</GridPane>
